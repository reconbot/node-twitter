{
  "name": "twitter",
  "version": "v1.2.5",
  "description": "Twitter API client library for node.js",
  "keywords": [
    "twitter",
    "streaming",
    "oauth"
  ],
  "homepage": "https://github.com/desmondmorris/node-twitter",
  "author": "Desmond Morris <hi@desmondmorris.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/desmondmorris/node-twitter"
  },
  "scripts": {
    "test": "npm run lint && mocha",
    "lint": "jshint test/*.js lib/*.js && echo Lint passed."
  },
  "dependencies": {
    "deep-extend": "^0.4.1",
    "request": "^2.51.0"
  },
  "devDependencies": {
    "jshint": "^2.6.0",
    "mocha": "^3.1.2",
    "nock": "^9.0.2"
  },
  "main": "./lib/twitter",
  "jshintConfig": {
    "boss": true,
    "node": true,
    "strict": true,
    "newcap": false,
    "undef": true,
    "unused": true,
    "onecase": true,
    "lastsemic": true,
    "indent": 2,
    "globals": {
      "escape": true,
      "describe": true,
      "it": true,
      "before": true
    }
  }
}
